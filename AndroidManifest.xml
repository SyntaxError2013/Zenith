    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="in.co.sdslabs.managecontacts.Homepage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="in.co.sdslabs.managecontacts.firstsheet" />
        <activity android:name="in.co.sdslabs.managecontacts.firstbackhand" />
        <activity android:name="in.co.sdslabs.managecontacts.add" />
        <activity android:name="in.co.sdslabs.managecontacts.massmessage" />
        <activity android:name="in.co.sdslabs.managecontacts.secondsheet" />
        <activity android:name="in.co.sdslabs.managecontacts.thirdsheet" />
        <activity android:name="in.co.sdslabs.managecontacts.add2" />
        <activity android:name="in.co.sdslabs.managecontacts.add3" />
        <activity android:name="in.co.sdslabs.managecontacts.massmessage2" />
        <activity android:name="in.co.sdslabs.managecontacts.massmessage3" />
        <activity android:name="in.co.sdslabs.managecontacts.secondbackhand" />
        <activity android:name="in.co.sdslabs.managecontacts.thirdbackhand" />
